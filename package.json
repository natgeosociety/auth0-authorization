{
  "name": "@cyrusbio/auth0-authorization",
  "version": "1.0.9",
  "description": "Auth0 Authorization Extension API client library",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "dependencies": {
    "@types/jsonwebtoken": "8.3.0",
    "@types/node-fetch": "2.1.4",
    "jsonwebtoken": "8.4.0",
    "node-fetch": "2.3.0"
  },
  "devDependencies": {
    "ava": "1.2.1",
    "npm-run-all": "4.1.5",
    "nyc": "13.3.0",
    "ts-node": "7.0.1",
    "typescript": "3.2.2"
  },
  "scripts": {
    "pretestbuild": "rm -rf test",
    "testbuild": "tsc -p tsconfig.test.json --outDir test",
    "testbuild:watch": "tsc -p tsconfig.test.json --outDir test -w",
    "unit": "nyc ava test/index.spec.js",
    "unit:watch": "ava test/index.spec.js -w",
    "test": "run-s testbuild unit",
    "test:watch": "run-p testbuild:watch unit:watch",
    "prepublish": "rm -rf lib; tsc --rootDir src --outDir lib --declaration"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/CyrusBiotechnology/auth0-authorization"
  },
  "keywords": [
    "auth0",
    "authorization"
  ]
}
